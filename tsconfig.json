{
    "compilerOptions": {
      "target": "ES2015",
      "module": "CommonJS",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "static",
      "sourceMap": false,
      "moduleResolution": "node",
      "noImplicitAny": false,
      "resolveJsonModule": true,
      "experimentalDecorators": true
    }
}